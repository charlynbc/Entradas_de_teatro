{
  "name": "entradas-de-teatro-root",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently \"npm run dev:api\" \"npm run dev:app\"",
    "dev:api": "cd teatro-tickets-backend && npm start",
    "dev:app": "cd baco-teatro-app && npm run web",
    "install:all": "npm install && cd teatro-tickets-backend && npm install && cd ../baco-teatro-app && npm install"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
